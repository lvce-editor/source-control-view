{
  "name": "@lvce-editor/source-control-worker",
  "version": "0.0.0-dev",
  "description": "Source Control Worker",
  "keywords": [
    "Lvce Editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvce-editor/source-control-worker.git"
  },
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "main": "src/sourceControlMain.ts",
  "scripts": {
    "dev": "cd ../../ && npm run dev",
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 70,
        "lines": 80
      }
    }
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@lvce-editor/assert": "^1.4.0",
    "@lvce-editor/constants": "^1.26.0",
    "@lvce-editor/i18n": "^1.1.0",
    "@lvce-editor/rpc": "^4.20.0",
    "@lvce-editor/rpc-registry": "^3.10.0",
    "@lvce-editor/verror": "^1.7.0",
    "@lvce-editor/viewlet-registry": "^1.4.0",
    "@lvce-editor/virtual-dom-worker": "^3.2.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5"
  }
}
